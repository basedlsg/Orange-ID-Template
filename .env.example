# Using SQLite database by default (recommended for development)
USE_SQLITE=true

# Optional: Custom session secret (set this to something unique in production)
SESSION_SECRET=orange-auth-template-secret

# Debug settings (uncomment to enable)
# DEBUG_SQLITE=true

# PostgreSQL connection string (uncomment to use PostgreSQL instead of SQLite)
# DATABASE_URL=postgresql://username:password@host:port/database
